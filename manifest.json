{
    "name": "Tab groups manager",
    "description": "Make tab groups and switch between them",
    "version": "0.1.0",
    "manifest_version": 3,
    "icons": {
        "16": "/assets/images/icon16.png",
        "32": "/assets/images/icon32.png",
        "48": "/assets/images/icon48.png",
        "128": "/assets/images/icon128.png"
    },
    "background": {
        "service_worker": "bgWrapper.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/assets/images/icon16.png",
            "32": "/assets/images/icon32.png",
            "48": "/assets/images/icon48.png",
            "128": "/assets/images/icon128.png"
        }
    },
    "permissions": [
        "storage",
        "activeTab",
        "tabs"
    ]
}